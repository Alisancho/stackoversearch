<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %m%n</pattern>
        </encoder>
    </appender>

    <logger name="aia.deploy" level="INFO" />
    <root level="INFO">
        <appender-ref ref="STDOUT" />
        <!--        <appender-ref ref="stash" />-->
    </root>
</configuration>

